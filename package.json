{
  "name": "am-bot",
  "keywords": [],
  "private": true,
  "main": "index.js",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "preinstall": "npx only-allow pnpm",
    "reset": "am-scripts resetNodeModules",
    "turbo:clean": "rimraf .turbo"
  },
  "devDependencies": {
    "@anmeng/utils": "^1.0.11",
    "@types/node": "^18.15.5",
    "chalk": "^5.2.0",
    "cross-spawn": "^7.0.3",
    "eslint-config-custom": "workspace:*",
    "fast-glob": "^3.2.12",
    "husky": "^8.0.3",
    "next": "^13.2.4",
    "prettier": "latest",
    "prettier-plugin-organize-imports": "3.0.1",
    "prettier-plugin-packagejson": "^2.2.18",
    "ts-node": "^10.0.0",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "turbo": "latest",
    "typescript": "^4.9.0",
    "webpack": "^5.75.0",
    "zx": "^7.2.1"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "lint-staged": {
    "*.{jsx,less,md,json}": [
      "prettier --cache --write"
    ],
    "*.ts?(x)": [
      "prettier --cache --parser=typescript --write"
    ]
  },
  "packageManager": "pnpm@7.25.0"
}
